<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TestMauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TestMauiApp1"
    Shell.FlyoutBehavior="Disabled"
    xmlns:views="clr-namespace:TestMauiApp1.Views">

    <TabBar>
            <ShellContent
                 Title="Accueil"
                 ContentTemplate="{DataTemplate local:MainPage}"
                 Icon="icon_notes" />
                    <ShellContent
                 Title="A Propos"
                 ContentTemplate="{DataTemplate local:APropos}"
                 Icon="icon_about" />
        <ShellContent 
                 Title="Information"
                 ContentTemplate="{DataTemplate views:vPhone}"
                 Icon="info_icon"/>
        <ShellContent
                 Title="Geolocalisation"
                 ContentTemplate="{DataTemplate views:vGeolocalisation}"
                 Icon="icon_geo"/>


    </TabBar>
    
    

</Shell>
